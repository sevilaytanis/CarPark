@model IEnumerable<CarPark.Entities.Concrete.Employee>

@{
    ViewData["Title"] = "GetEmployeesByAges";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Personel Listesi</h1>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Create Date</th>
        <th>UserName</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
            <td>@item.CreateDate</td>
            <td>@item.Username</td>
        </tr>
    }
</table>


